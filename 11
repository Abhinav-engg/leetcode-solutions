# 11.Container with most water

def maxArea(self, height: List[int]) -> int:
        left = 0
        right = len(height) - 1
        maxArea = 0
        while right > left:
            leftHeight = height[left]
            rightHeight = height[right]
        
            if leftHeight < rightHeight:
                currArea = leftHeight * (right - left)
                left += 1
            else:
                currArea = rightHeight * (right - left)
                right -= 1
        
            if currArea > maxArea:
                maxArea = currArea
        return maxArea
